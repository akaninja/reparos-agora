<h1>Editar perfil de prestador</h1>
<%= flash[:alert] %>
<%= form_for @contractor do |f| %>
  <p><%= f.label :name, 'Nome' %></p>
  <p><%= f.text_field :name %></p>
  <p><%= f.label :category_id, 'Categorias' %></p>
  <p><%= f.collection_select :category_id, @categories, :id, :name %></p>
  <p><%= f.submit 'Atualizar'%>
<% end %>