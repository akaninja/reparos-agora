<%= form_for([@contractor, @estimate]) do |f| %>
  <%= f.label :title %> <br>
  <%= f.text_field :title %><br>
  <%= f.label :description %> <br>
  <%= f.text_area :description %><br>
  <%= f.label :location %> <br>
  <%= f.text_field :location %><br>
  <%= f.label :service_date %> <br>
  <%= f.text_field :service_date %><br>
  <%= f.label :day_shift %> <br>
  <%= f.select :day_shift, Estimate::SHIFTS %><br>
  <%= f.label :photo %><br>
  <%= f.file_field :photo %><br>
  <%= f.submit %>
<%end%>

